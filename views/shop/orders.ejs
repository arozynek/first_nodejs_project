<%- include('../includes/head.ejs') %>
</head>
<body>
  <%- include('../includes/header.ejs') %>
  <h1><%= pageTitle %></h1>
  <% if (orders.length <= 0) { %>
  <h1>It's nothing here!</h1>
  <% } else { %>
    <ul>
      <% orders.forEach(order => { %>
        <li>
          <h1># <%= order.id %></h1>
          <ul><% order.products.forEach(product => { %> 
            <li>
              <%= product.title %> : <%=product.orderItem.quantity %>
            </li>
            <% }) %> 
          </ul>
        </li>
        <% }) %>
    </ul>
  <% } %>
  <%- include('../includes/end.ejs') %>